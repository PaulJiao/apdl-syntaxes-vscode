{
    "name": "apdl",
    "displayName": "APDL",
    "description": "APDL(ANSYS Scripts) Language Support for VS Code.",
    "version": "0.0.1",
    "publisher": "shenbo",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "apdl",
            "aliases": ["APDL (ANSYS)", "apdl"],
            "extensions": [".mac",".ans",".ansys.txt",".ans.txt",".ansys",".apdl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "apdl",
            "scopeName": "source.apdl",
            "path": "./syntaxes/apdl.tmLanguage"
        }]
    }
}